{
	"name": "sapglparquet",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sapska1json",
						"type": "DatasetReference"
					},
					"name": "sapska1"
				},
				{
					"dataset": {
						"referenceName": "sapskatjson",
						"type": "DatasetReference"
					},
					"name": "sapskat"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sapska1parquet",
						"type": "DatasetReference"
					},
					"name": "sapska1parquet"
				},
				{
					"dataset": {
						"referenceName": "sapskatparquet1",
						"type": "DatasetReference"
					},
					"name": "sapskatparquet"
				}
			],
			"transformations": [
				{
					"name": "sapska1select"
				},
				{
					"name": "sapskatselect"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'singleDocument',",
				"     partitionBy('hash', 1)) ~> sapska1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'singleDocument') ~> sapskat",
				"sapska1 select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sapska1select",
				"sapskat select(partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sapskatselect",
				"sapska1select sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['sapska1parquet'],",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sapska1parquet",
				"sapskatselect sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['sapskatparquet'],",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sapskatparquet"
			]
		}
	}
}